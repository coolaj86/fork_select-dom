		"browserify": "^17.0.0",
		"esmify": "^2.1.1",
		"tape": "^5.6.4",
		"tape-run": "^10.0.0",
		"tsd": "^0.28.1",
		"typescript": "^5.1.6",
		"xo": "^0.54.2"
	},
	"engines": {
		"node": ">=16"
	}
